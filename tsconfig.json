{
	"compilerOptions": {
		"moduleResolution": "Node",
		"module": "ES2020",
		"target": "ES2020",
		"strict": true,
		"removeComments": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"plugins": [
			{
				"transform": "typescript-rtti/dist/transformer"
			}
		]
	},
	"reflection": {
		"metadata": false,
		"debugMode": false
	}
}
